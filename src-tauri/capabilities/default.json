{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main", "about", "update", "settings"],
  "remote": {
    "urls": ["https://weread.qq.com/*"]
  },
  "permissions": [
    "core:default",
    "opener:default",
    "store:default",
    "window-state:default",
    "log:default",
    "shell:allow-open",
    "core:event:default",
    "core:window:default",
    "core:window:allow-set-size",
    "core:webview:default"
  ]
}
