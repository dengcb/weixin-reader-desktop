{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "微信阅读",
  "version": "0.2.0",
  "identifier": "com.dengcb.reader",
  "build": {
    "beforeDevCommand": "bun run build",
    "devUrl": null,
    "beforeBuildCommand": "bun run build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [],
    "security": {
      "csp": null,
      "capabilities": [
        "default"
      ]
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}